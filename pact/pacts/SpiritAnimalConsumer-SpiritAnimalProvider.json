      "request": {
        "method": "GET",
        "path": "/SpiritAnimal/10"
      },
      "response": {
        "body": {
          "colour": "CREDIT_CARD",
          "id": 10,
          "name": "GEM Visa"
        },
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "matchingRules": {
          "body": {
            "$.colour": {
